import java.util.*;
public class Main
{
  public static void main (String[]args)
  {
      
        Scanner scan = new Scanner (System.in);
        System.out.println("Digite um n√∫mero: ");
        int num = scan.nextInt();
        
        switch (num % 2) {
            
            case 0:
                System.out.println("par");
                break;
                
            case 1:
                System.out.println("impar");
                break;
                
            default:
                System.out.println("invalido");
                break;
        }
  }
}
